{
  "id": null,
  "uid": "rag-overview",
  "title": "RAG Pipeline Overview",
  "tags": [
    "rag",
    "overview"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Query Success Rate",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "(sum(rate(rag_query_success_total[5m])) / sum(rate(rag_query_total[5m]))) * 100",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 90,
                "color": "yellow"
              },
              {
                "value": 99,
                "color": "green"
              }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Average Query Latency",
      "gridPos": {
        "x": 6,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(rag_query_duration_seconds_bucket[5m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 5,
                "color": "yellow"
              },
              {
                "value": 10,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Database Status",
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "pg_up",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "value": 0,
              "text": "DOWN",
              "color": "red"
            },
            {
              "value": 1,
              "text": "UP",
              "color": "green"
            }
          ]
        }
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Backup Status",
      "gridPos": {
        "x": 18,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "rag_backup_success",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "value": 0,
              "text": "FAILED",
              "color": "red"
            },
            {
              "value": 1,
              "text": "OK",
              "color": "green"
            }
          ]
        }
      }
    },
    {
      "id": 5,
      "type": "graph",
      "title": "Query Rate (queries/sec)",
      "gridPos": {
        "x": 0,
        "y": 4,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "rate(rag_query_total[5m])",
          "legendFormat": "Total Queries",
          "refId": "A"
        },
        {
          "expr": "rate(rag_query_success_total[5m])",
          "legendFormat": "Successful",
          "refId": "B"
        },
        {
          "expr": "rate(rag_query_errors_total[5m])",
          "legendFormat": "Errors",
          "refId": "C"
        }
      ],
      "yaxes": [
        {
          "format": "reqps",
          "label": "Queries/sec"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "id": 6,
      "type": "graph",
      "title": "Query Latency Percentiles",
      "gridPos": {
        "x": 12,
        "y": 4,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(rag_query_duration_seconds_bucket[5m]))",
          "legendFormat": "P50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(rag_query_duration_seconds_bucket[5m]))",
          "legendFormat": "P95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, rate(rag_query_duration_seconds_bucket[5m]))",
          "legendFormat": "P99",
          "refId": "C"
        }
      ],
      "yaxes": [
        {
          "format": "s",
          "label": "Latency"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "id": 7,
      "type": "graph",
      "title": "Database Connections",
      "gridPos": {
        "x": 0,
        "y": 12,
        "w": 8,
        "h": 6
      },
      "targets": [
        {
          "expr": "pg_stat_database_numbackends{datname='vector_db'}",
          "legendFormat": "Active Connections",
          "refId": "A"
        },
        {
          "expr": "pg_settings_max_connections",
          "legendFormat": "Max Connections",
          "refId": "B"
        }
      ]
    },
    {
      "id": 8,
      "type": "graph",
      "title": "Database Size",
      "gridPos": {
        "x": 8,
        "y": 12,
        "w": 8,
        "h": 6
      },
      "targets": [
        {
          "expr": "pg_database_size_bytes{datname='vector_db'}",
          "legendFormat": "Database Size",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "bytes",
          "label": "Size"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "id": 9,
      "type": "graph",
      "title": "Retrieval Scores",
      "gridPos": {
        "x": 16,
        "y": 12,
        "w": 8,
        "h": 6
      },
      "targets": [
        {
          "expr": "rag_retrieval_score_avg",
          "legendFormat": "Average Score",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "short",
          "label": "Similarity Score",
          "min": 0,
          "max": 1
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "id": 10,
      "type": "graph",
      "title": "CPU Usage",
      "gridPos": {
        "x": 0,
        "y": 18,
        "w": 8,
        "h": 6
      },
      "targets": [
        {
          "expr": "(1 - avg(rate(node_cpu_seconds_total{mode='idle'}[5m]))) * 100",
          "legendFormat": "CPU %",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "percent",
          "label": "CPU",
          "min": 0,
          "max": 100
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "id": 11,
      "type": "graph",
      "title": "Memory Usage",
      "gridPos": {
        "x": 8,
        "y": 18,
        "w": 8,
        "h": 6
      },
      "targets": [
        {
          "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
          "legendFormat": "Memory %",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "percent",
          "label": "Memory",
          "min": 0,
          "max": 100
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "id": 12,
      "type": "graph",
      "title": "Disk Usage",
      "gridPos": {
        "x": 16,
        "y": 18,
        "w": 8,
        "h": 6
      },
      "targets": [
        {
          "expr": "(1 - (node_filesystem_avail_bytes{mountpoint='/'} / node_filesystem_size_bytes{mountpoint='/'})) * 100",
          "legendFormat": "Disk %",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "percent",
          "label": "Disk",
          "min": 0,
          "max": 100
        },
        {
          "format": "short"
        }
      ]
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m",
      "15m",
      "30m",
      "1h"
    ]
  }
}
