      "gridPos": {"x": 0, "y": 37, "w": 12, "h": 6},
      "targets": [{
        "expr": "rate(rag_llm_generation_duration_seconds_sum[1m]) / rate(rag_llm_generation_duration_seconds_count[1m])",
