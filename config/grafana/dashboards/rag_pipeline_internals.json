          "legendFormat": "Average"
        },
        {
          "expr": "rag_retrieval_score_p50",
          "refId": "B",
          "legendFormat": "p50"
        },
        {
          "expr": "rag_retrieval_score_p95",
          "refId": "C",
          "legendFormat": "p95"
        },
        {
          "expr": "rag_retrieval_score_p99",
          "refId": "D",
          "legendFormat": "p99"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0,
          "max": 1,
          "decimals": 3,
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "legend": {
          "showLegend": true,
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 16,
      "type": "stat",
      "title": "Top Retrieval Score",
      "description": "Highest similarity score",
      "gridPos": {"x": 12, "y": 22, "w": 6, "h": 4},
      "targets": [{
        "expr": "rag_retrieval_top_score",
