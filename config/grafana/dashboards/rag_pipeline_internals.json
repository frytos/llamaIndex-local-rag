{
  "id": null,
  "uid": "rag-pipeline-internals",
  "title": "RAG Pipeline Internals",
  "description": "Deep dive into RAG pipeline stages: embedding, retrieval, and LLM generation",
  "tags": ["rag", "pipeline", "performance"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h"]
  },
  "panels": [
    {
      "id": 1,
      "type": "row",
      "title": "Pipeline Overview",
      "gridPos": {"x": 0, "y": 0, "w": 24, "h": 1},
      "collapsed": false
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Total Query Time (Avg)",
      "description": "Average end-to-end query latency",
      "gridPos": {"x": 0, "y": 1, "w": 6, "h": 4},
      "targets": [{
        "expr": "rate(rag_query_duration_seconds_sum[5m]) / rate(rag_query_duration_seconds_count[5m])",
        "refId": "A",
        "legendFormat": "Query Duration"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 2, "color": "yellow"},
              {"value": 5, "color": "red"}
            ]
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto"
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Success Rate",
      "description": "Query success rate (last 5 minutes)",
      "gridPos": {"x": 6, "y": 1, "w": 6, "h": 4},
      "targets": [{
        "expr": "(sum(rate(rag_query_success_total[5m])) / sum(rate(rag_query_total[5m]))) * 100",
        "refId": "A"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "red"},
              {"value": 95, "color": "yellow"},
              {"value": 99, "color": "green"}
            ]
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Queries/Second",
      "description": "Query throughput",
      "gridPos": {"x": 12, "y": 1, "w": 6, "h": 4},
      "targets": [{
        "expr": "rate(rag_query_total[1m])",
        "refId": "A"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "qps",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "blue"}
            ]
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "id": 5,
      "type": "stat",
      "title": "Total Queries",
      "description": "Total queries in time range",
      "gridPos": {"x": 18, "y": 1, "w": 6, "h": 4},
      "targets": [{
        "expr": "rag_query_total",
        "refId": "A"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [{"value": 0, "color": "blue"}]
          }
        }
      },
      "options": {
        "colorMode": "value"
      }
    },
    {
      "id": 6,
      "type": "row",
      "title": "Pipeline Stage Breakdown",
      "gridPos": {"x": 0, "y": 5, "w": 24, "h": 1},
      "collapsed": false
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Stage Duration Over Time",
      "description": "Time spent in each pipeline stage",
      "gridPos": {"x": 0, "y": 6, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "rate(rag_pipeline_stage_duration_seconds_sum{stage=\"embedding\"}[1m]) / rate(rag_pipeline_stage_duration_seconds_count{stage=\"embedding\"}[1m])",
          "refId": "A",
          "legendFormat": "Embedding"
        },
        {
          "expr": "rate(rag_pipeline_stage_duration_seconds_sum{stage=\"retrieval\"}[1m]) / rate(rag_pipeline_stage_duration_seconds_count{stage=\"retrieval\"}[1m])",
          "refId": "B",
          "legendFormat": "Retrieval"
        },
        {
          "expr": "rate(rag_pipeline_stage_duration_seconds_sum{stage=\"generation\"}[1m]) / rate(rag_pipeline_stage_duration_seconds_count{stage=\"generation\"}[1m])",
          "refId": "C",
          "legendFormat": "Generation"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20,
            "showPoints": "never",
            "stacking": {
              "mode": "normal"
            }
          }
        },
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "Embedding"},
            "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "blue"}}]
          },
          {
            "matcher": {"id": "byName", "options": "Retrieval"},
            "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}]
          },
          {
            "matcher": {"id": "byName", "options": "Generation"},
            "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "orange"}}]
          }
        ]
      },
      "options": {
        "legend": {
          "showLegend": true,
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
    {
      "id": 8,
      "type": "piechart",
      "title": "Stage Duration Distribution (Avg)",
      "description": "Percentage of time spent in each stage",
      "gridPos": {"x": 12, "y": 6, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "sum(rate(rag_pipeline_stage_duration_seconds_sum{stage=\"embedding\"}[5m]))",
          "refId": "A",
          "legendFormat": "Embedding"
        },
        {
          "expr": "sum(rate(rag_pipeline_stage_duration_seconds_sum{stage=\"retrieval\"}[5m]))",
          "refId": "B",
          "legendFormat": "Retrieval"
        },
        {
          "expr": "sum(rate(rag_pipeline_stage_duration_seconds_sum{stage=\"generation\"}[5m]))",
          "refId": "C",
          "legendFormat": "Generation"
        }
      ],
      "options": {
        "pieType": "donut",
        "legend": {
          "showLegend": true,
          "displayMode": "table",
          "placement": "right",
          "values": ["value", "percent"]
        },
        "tooltip": {
          "mode": "single"
        }
      }
    },
    {
      "id": 9,
      "type": "row",
      "title": "Embedding Performance",
      "gridPos": {"x": 0, "y": 14, "w": 24, "h": 1},
      "collapsed": false
    },
    {
      "id": 10,
      "type": "gauge",
      "title": "Embedding Throughput",
      "description": "Embeddings generated per second",
      "gridPos": {"x": 0, "y": 15, "w": 6, "h": 6},
      "targets": [{
        "expr": "rag_embeddings_per_second",
        "refId": "A"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "min": 0,
          "max": 200,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "red"},
              {"value": 30, "color": "yellow"},
              {"value": 60, "color": "green"}
            ]
          }
        }
      },
      "options": {
        "showThresholdLabels": true,
        "showThresholdMarkers": true
      }
    },
    {
      "id": 11,
      "type": "stat",
      "title": "Embedding Duration (Avg)",
      "description": "Average embedding operation time",
      "gridPos": {"x": 6, "y": 15, "w": 6, "h": 6},
      "targets": [{
        "expr": "rate(rag_embedding_duration_seconds_sum[5m]) / rate(rag_embedding_duration_seconds_count[5m])",
        "refId": "A"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 0.1, "color": "yellow"},
              {"value": 0.5, "color": "red"}
            ]
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "id": 12,
      "type": "gauge",
      "title": "Batch Utilization",
      "description": "Embedding batch size utilization",
      "gridPos": {"x": 12, "y": 15, "w": 6, "h": 6},
      "targets": [{
        "expr": "rag_embedding_batch_utilization_percent",
        "refId": "A"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "red"},
              {"value": 50, "color": "yellow"},
              {"value": 80, "color": "green"}
            ]
          }
        }
      },
      "options": {
        "showThresholdLabels": true,
        "showThresholdMarkers": true
      }
    },
    {
      "id": 13,
      "type": "stat",
      "title": "Embedding Errors",
      "description": "Total embedding errors",
      "gridPos": {"x": 18, "y": 15, "w": 6, "h": 6},
      "targets": [{
        "expr": "rag_embedding_errors_total",
        "refId": "A"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 1, "color": "yellow"},
              {"value": 10, "color": "red"}
            ]
          }
        }
      },
      "options": {
        "colorMode": "background"
      }
    },
    {
      "id": 14,
      "type": "row",
      "title": "Retrieval Performance",
      "gridPos": {"x": 0, "y": 21, "w": 24, "h": 1},
      "collapsed": false
    },
    {
      "id": 15,
      "type": "timeseries",
      "title": "Retrieval Scores Over Time",
      "description": "Average, p50, p95, p99 retrieval similarity scores",
      "gridPos": {"x": 0, "y": 22, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "rag_retrieval_score_avg",
