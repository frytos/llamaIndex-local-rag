          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "id": 17,
      "type": "stat",
      "title": "Score Variance",
      "description": "Variance in retrieval scores",
      "gridPos": {"x": 18, "y": 22, "w": 6, "h": 4},
      "targets": [{
        "expr": "rag_retrieval_score_variance",
