            "drawStyle": "line",
            "fillOpacity": 10
          }
        },
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "Tokens Available"},
            "properties": [
              {"id": "custom.fillOpacity", "value": 0},
              {"id": "custom.lineStyle", "value": {"dash": [10, 10], "fill": "dash"}}
            ]
          }
        ]
      }
    },
    {
