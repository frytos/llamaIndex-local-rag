{
  "id": null,
  "uid": "rag-application",
  "title": "RAG Application Metrics",
  "tags": ["rag", "application", "performance"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Total Queries",
      "gridPos": {"x": 0, "y": 0, "w": 6, "h": 4},
      "targets": [
        {
          "expr": "rag_query_total",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        }
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Query Success Rate",
      "gridPos": {"x": 6, "y": 0, "w": 6, "h": 4},
      "targets": [
        {
          "expr": "(rag_query_success_total / rag_query_total) * 100",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "red"},
              {"value": 90, "color": "yellow"},
              {"value": 99, "color": "green"}
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Average Query Time",
      "gridPos": {"x": 12, "y": 0, "w": 6, "h": 4},
      "targets": [
        {
          "expr": "rag_query_duration_seconds_sum / rag_query_duration_seconds_count",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 5, "color": "yellow"},
              {"value": 10, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Cache Hit Rate",
      "gridPos": {"x": 18, "y": 0, "w": 6, "h": 4},
      "targets": [
        {
          "expr": "rag_cache_hit_rate * 100",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "red"},
              {"value": 50, "color": "yellow"},
              {"value": 80, "color": "green"}
            ]
          }
        }
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Query Rate (queries/min)",
      "gridPos": {"x": 0, "y": 4, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "rate(rag_query_total[1m]) * 60",
          "refId": "A",
          "legendFormat": "Total Queries",
          "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
        },
        {
          "expr": "rate(rag_query_success_total[1m]) * 60",
          "refId": "B",
          "legendFormat": "Successful",
          "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
        },
        {
          "expr": "rate(rag_query_errors_total[1m]) * 60",
          "refId": "C",
          "legendFormat": "Errors",
          "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20,
            "lineWidth": 2
          }
        }
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Query Duration",
      "gridPos": {"x": 12, "y": 4, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "rag_query_duration_seconds_sum / rag_query_duration_seconds_count",
          "refId": "A",
          "legendFormat": "Average",
          "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20,
            "lineWidth": 2
          }
        }
      }
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Retrieval Metrics",
      "gridPos": {"x": 0, "y": 12, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "rag_retrieval_score_avg",
          "refId": "A",
          "legendFormat": "Average Score",
          "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
        },
        {
          "expr": "rate(rag_retrieval_documents_total[1m])",
          "refId": "B",
          "legendFormat": "Documents/sec",
          "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "lineWidth": 2
          }
        }
      }
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "Cache Performance",
      "gridPos": {"x": 12, "y": 12, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "rate(rag_cache_hits_total[1m])",
          "refId": "A",
          "legendFormat": "Hits/sec",
          "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
        },
        {
          "expr": "rate(rag_cache_misses_total[1m])",
          "refId": "B",
          "legendFormat": "Misses/sec",
          "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
        },
        {
          "expr": "rag_cache_hit_rate * 100",
          "refId": "C",
          "legendFormat": "Hit Rate %",
          "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "lineWidth": 2
          }
        }
      }
    },
    {
      "id": 9,
      "type": "stat",
      "title": "Average Retrieval Score",
      "gridPos": {"x": 0, "y": 20, "w": 6, "h": 4},
      "targets": [
        {
          "expr": "rag_retrieval_score_avg",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 3,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "red"},
              {"value": 0.5, "color": "yellow"},
              {"value": 0.7, "color": "green"}
            ]
          }
        }
      }
    },
    {
      "id": 10,
      "type": "stat",
      "title": "Total Documents Retrieved",
      "gridPos": {"x": 6, "y": 20, "w": 6, "h": 4},
      "targets": [
        {
          "expr": "rag_retrieval_documents_total",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        }
      }
    },
    {
      "id": 11,
      "type": "stat",
      "title": "Total Errors",
      "gridPos": {"x": 12, "y": 20, "w": 6, "h": 4},
      "targets": [
        {
          "expr": "rag_query_errors_total",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 1, "color": "yellow"},
              {"value": 10, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "id": 12,
      "type": "stat",
      "title": "Database Rows",
      "gridPos": {"x": 18, "y": 20, "w": 6, "h": 4},
      "targets": [
        {
          "expr": "rag_db_rows_total",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        }
      }
    }
  ]
}
